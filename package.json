{
  "name": "react-flexible-workbench",
  "version": "5.4.2",
  "description": "Provides a flexible workspace UI for single-page web apps based on GoldenLayout",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:lib",
    "build:lib": "tsc && copyfiles -u 1 src/*.css lib",
    "dist": "npm run build && npm run dist:file && npm run dist:minify",
    "dist:file": "webpack --config webpack.build.js",
    "dist:minify": "MINIFY=1 npm run dist:file",
    "clean": "rimraf lib dist",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "preversion": "npm test",
    "start": "webpack serve --config webpack.config.js",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "test": "npm run build"
  },
  "author": "Tam√°s Nepusz <ntamas@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ntamas/react-flexible-workbench.git"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.6",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.7.10",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.20",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.2.0",
    "delay": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jsdoc": "^36.0.8",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jquery": "3.6.0",
    "react": "^17",
    "react-dom": "^17",
    "react-shapeshifter": "^1.0.10",
    "react-spinners": "^0.11.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  },
  "dependencies": {
    "arrive": "^2.4.1",
    "eventemitter3": "^4.0.7",
    "golden-layout": "^1.5.9",
    "javascript-detect-element-resize": "^0.5.3",
    "lodash-es": "^4.17.21",
    "mutation-observer": "^1.0.3",
    "prop-types": "^15.7.2",
    "react-badger": "^2.0.7",
    "react-beautiful-dnd": "^13.1.0"
  },
  "peerDependencies": {
    "jquery": "^3.6.0",
    "react": "^16.3.2 || ^17",
    "react-dom": "^16.3.2 || ^17"
  }
}
